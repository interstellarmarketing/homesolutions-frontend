{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "@cloudflare/workers-types/experimental"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@actions/*": [
        "src/actions/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@consts/*": [
        "src/consts/*"
      ],
      "@models/*": [
        "src/models/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}