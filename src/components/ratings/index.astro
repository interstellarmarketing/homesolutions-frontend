---
interface Props {
    src: string;
    alt?: string;
}

const { src, alt = "" } = Astro.props;
---

<div class="p-3 bg-gray-50 rounded-lg flex flex-col justify-center items-center gap-2.5">
    <img 
        src={src} 
        alt={alt}
        class="w-28 h-[65px] object-contain" 
    />
</div>
